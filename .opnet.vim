" global variables
syntax match opnetEnvVar		"\<[A-Z][a-z][a-z]*E_[A-Za-z0-9_]*\>"
syntax match opnetSharedVar		"\<[A-Z][a-z][a-z]*S_[A-Za-z0-9_]*\>"
syntax match opnetPkgIntVar		"\<[A-Z][a-z][a-z]*I_[A-Za-z0-9_]*\>"
syntax match opnetOldLocalVar	"\<[A-Z][a-z][a-z]*L_[A-Za-z0-9_]*\>"

" functions
syntax match opnetFunc		"\<\(An\|Annot\|As\|Dbr\|Em\|Ema\|Et\|Ets\|Ex\|Fi\|Flp\|Gap\|Gfx\|M3\|Mdep\|Mmi\|Mmirel\|Nd\|Nmx\|Nt\|Obj\|Op\|Orb\|Os\|Ov\|Ovd\|Ovimport\|Pb\|Pk\|Pr\|Selfdesc\|Slib\|Stat\|Tmm\|Traf\|Ve\|Vos\)_[A-Za-z0-9_]*\>"
syntax match opnetIntFunc		"\<\(adp\|an\|annot\|bbox\|carto\|em\|ema\|esd\|esys\|et\|ets\|ex\|fi\|in\|mdep\|mdl\|mmi\|nd\|nt\|obj\|os\|ovd\|pb\|pbsp\|pj\|pk\|pm\|pr\|prg\|prim\|ps\|rect\|repos\|sd\|selfdesc\|sim\|stat\|tmm\|vg\|vos\)_[A-Za-z0-9_]*\>"

" KPs, the easy way
" syntax match opnetKP		"\<op_[a-z0-9_]*\>"
" KPs, tighter
